{
  "name": "lanchao-ui",
  "version": "0.0.3",
  "description": "这是一个ui基础库",
  "main": "dist/index.js",
  "scripts": {
    "start": "npx parcel index.html --no-cache",
    "dev-test": "npx parcel watch test/* --no-cache & karma start",
    "build" :"npx parcel build index.js --no-cache --no-minify",
    "test": "npx parcel build test/* --no-cache --no-minify && karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziwei3749/lanchao-ui.git",
    "start": "./node_modules/.bin/parcel index.html"
  },
  "keywords": [
    "vue",
    "ui"
  ],
  "author": "吕子威",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ziwei3749/lanchao-ui/issues"
  },
  "homepage": "https://github.com/ziwei3749/lanchao-ui#readme",
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "dependencies": {
    "hoek": "^4.2.1",
    "tunnel-agent": "^0.6.0",
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.1.1",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "karma": "^2.0.5",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.7",
